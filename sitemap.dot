subgraph cluster_0 {
label="Legend";
node [shape=box,group="legend"];
"User Docs\n(handwritten html)" [fillcolor="#d6d583"];
"Developer Docs\n(wiki & google code)" [fillcolor="#c3d9ff"];
"Generated Docs\n(generated html)" [fillcolor="#dddddd"];
}

